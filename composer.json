{
    "name": "vitexsoftware/debs2sql",
    "description": "Tool used to setup AbraFlexi multiinstance",
    "version": "0.1",
    "type": "project",
    "license": "MIT",
    "authors": [
        {
            "name": "Vítězslav Dvořák",
            "email": "info@vitexsoftware.cz"
        }
    ],
    "minimum-stability": "dev",
    "require": {
        "vitexsoftware/ease-core": "^1.48",
        "vitexsoftware/ease-fluentpdo": "^1.4",
        "vitexsoftware/ease-twbootstrap5": "^1.2",
        "league/commonmark": "^2.7@dev"
    },
    "require-dev": {
        "robmorgan/phinx": "*",
        "phpunit/phpunit": "*",
        "phpstan/phpstan": "*",
        "friendsofphp/php-cs-fixer": "^3.85",
        "ergebnis/composer-normalize": "^2.47",
        "ergebnis/php-cs-fixer-config": "^6.51"
    },

    "autoload": {
        "psr-4": {
            "DebToSQL\\": "src/DebToSQL/",
            "DebToSQL\\Ui\\": "src/DebToSQL/Ui"
        }
    },
    "config": {
        "allow-plugins": {
            "ergebnis/composer-normalize": true
        }
    }
}
